#inclide <stdio.h>

int main(){
 printf("今日は%d日です。\n");/'変数が指定されていない。/'
 printf("今日は%d日です。\n" i);/'メッセージと変数の間に,がない。/'
 printf("今日は%d日です。\n",i);
 printf("今日は17日です。\n");
 printf("今日は%d日です。\n",17);
 printf("今日は%d日です。\n",i+1);
 printf("%d日は%d度でした。\n",i);/'%dと変数の数がリンクしない。/'
 printf("%d日は%d度でした。\n,j",i);/'”の位置がおかしいのと、変数の数が%dとあっていない/'
 printf("%d日は%d度でした。\n",j,i);
 printf(%d日は%d度でした。,j,i);/'日本語の位置にコメント設定されていない。/'

				   return 0;
}
