#include <stdio.h>
int main()
{
  double x = 31.4;
  int i = 29;

  printf("認定単位数は%05dです\n", i);
  printf("認定単位数は\t%-dです\n", i);
  printf("認定単位数は%dです\n", i);
  printf("認定単位数は%dです\n", i);
  printf("総合点数は%0.3f点です\n", x);
  printf("総合点数は%0.6f点です\n", x);
  printf("総合点数は%e点です\n", x);
  printf("総合点数は%E点です\n", x); 

  return 0;  
}
